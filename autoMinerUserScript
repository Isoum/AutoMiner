// ==UserScript==
// @name AutoMiner
// @version v 0.1
// @description Userscript to load AutoMiner written by Isoum
// @author Isoum
// @include http://www.diamondhunt.co/
// @include http://www.diamondhunt.co/index.php?donorAch
// @grant none
// ==/UserScript==

(function () {
    var js = document.createElement('script');
    js.setAttribute('type', 'text/javascript');
    js.setAttribute('id', 'autoMinerScript');
    js.setAttribute('src', 'https://raw.githubusercontent.com/Isoum/AutoMiner/master/autoMiner.js');
    document.head.appendChild(js);
}());
